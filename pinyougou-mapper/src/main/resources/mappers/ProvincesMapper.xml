<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pinyougou.mapper.ProvincesMapper">

	<select id="findOne" resultType="Provinces">
		SELECT * FROM tb_provinces WHERE provinceid = #{provinceId}
	</select>

	<select id="findAllProvinces" resultType="Provinces">
		SELECT * FROM tb_provinces
	</select>
</mapper>